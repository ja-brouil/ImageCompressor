{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-task:scss-compile": "node-sass-chokidar --source-map true scss/ -o css/",
    "build-task:autoprefixer": "postcss dist/css/*.css --use autoprefixer -d dist/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'scss/*.scss' -c 'npm run build-task:scss-compile'",
    "dev": "npm-run-all -p sass:*"
  },
  "repository": "https://github.com/ja-brouil/ImageCompressor",
  "keywords": [
    "Electron",
    "jpeg",
    "compression"
  ],
  "author": "James Brouillet",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.5",
    "electron-rebuild": "^1.8.4"
  },
  "dependencies": {
    "autoprefixer": "^9.4.7",
    "chokidar-cli": "^1.2.1",
    "gm": "^1.23.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.1",
    "sharp": "^0.21.3"
  }
}
